<h1>Listing Entries</h1>

<% for entry in @entries: %>
<div class="item" data-id="<%= entry.id %>">
  <a data-type="show"><%= "#{entry.year}-#{entry.month}-#{entry.day}" %></a>
  <a data-type="edit">Edit</a> 
  <a data-type="destroy">Destroy</a>
</div>
<% end %>

<p><a data-type="new">New Entry</a></p>